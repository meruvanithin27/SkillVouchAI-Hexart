{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": false,
    "checkJs": false,
    "noEmit": true,
    "strict": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "suppressImplicitAnyIndexErrors": true,
    "noStrictGenericChecks": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strictBindCallApply": false,
    "strictPropertyInitialization": false,
    "noImplicitThis": false,
    "alwaysStrict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noResolve": true,
    "noLib": true,
    "isolatedModules": true,
    "noEmitOnError": false
  },
  "include": [],
  "exclude": [
    "**/*",
    "*.js",
    "*.jsx",
    "*.ts",
    "*.tsx",
    "server.js",
    "Backend/server.js",
    "Backend/**/*.js",
    "Frontend/**/*.js",
    "Frontend/**/*.jsx",
    "node_modules",
    "dist",
    "build"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "ts-node": {
    "esm": false
  }
}
